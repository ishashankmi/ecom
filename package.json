    "@tanstack/react-query": "^5.56.2",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-multi-carousel": "^2.8.5",
    "react-redux": "^9.1.2",
